{
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "html.filetypes": [
    "html",
    "ejs",
    "javascript",
    "typescript",
    "js",
    "jsx",
    "ftl"
  ],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.diagnostic.virtualText": true,
  "cSpell.userWords": [
    "Behaviours",
    "COVID",
    "Clwster",
    "DVLA",
    "Favourite",
    "Kieran's",
    "Tantrwm",
    "Unmount",
    "dogeer",
    "prioritise"
  ],
  "snippets.userSnippetsDirectory": "~/.config/nvim/snips",
  "eslint.enable": true,
  "python.pythonPath": "python",
  "java.configuration.runtimes": [
    {
      "path": "/Library/Java/JavaVirtualMachines/jdk-18.jdk",
      "default": true
    }
  ],
  "rust-analyzer.inlayHints.enable": true,
  "rust-analyzer.diagnostics.disabled": ["macro-error", "unresolved-proc-macro"]
}
