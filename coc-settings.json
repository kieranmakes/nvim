{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.diagnostic.virtualText": true,
  "cSpell.userWords": ["Unmount"],
  "eslint.enable": false
}
