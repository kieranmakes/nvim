{
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.diagnostic.virtualText": true,
  "cSpell.userWords": [
    "Behaviours",
    "COVID",
    "Clwster",
    "DVLA",
    "Favourite",
    "Kieran's",
    "Tantrwm",
    "Unmount",
    "dogeer",
    "prioritise"
  ],
  "snippets.userSnippetsDirectory": "~/.config/nvim/snips",
  "eslint.enable": true
}
